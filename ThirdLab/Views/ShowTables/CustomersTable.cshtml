
@{
    ViewBag.Title = "CustomersTable";
}

@model IEnumerable<ThirdLab.Models.Customers>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h3>Клиенты</h3>
    <table>
        <tr class="header">
            <td><p>Имя</p></td>
        </tr>
        @foreach (ThirdLab.Models.Customers b in Model)
        {
            <tr>
                <td><p>@b.Name</p></td>
            </tr>
        }
    </table>
</div>

